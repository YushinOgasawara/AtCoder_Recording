{
    "label": "c++ build for AtCoder",
    "type": "shell",
    "command": "g++",
    "args": [
      "-g",
      "-O0",
      "${file}",
      "-o",
      "${fileDirname}/${fileBasenameNoExtension}"
    ],
    "options": {
      "cwd": "${fileDirname}"
    },
    "group": {
      "kind": "build",
      "isDefault": true
    }
  }